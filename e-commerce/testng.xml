<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<exclude name="smoke"/>
			<include name="sanity"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="utils.ExtentReport"></listener>
	</listeners>
	<test thread-count="5" name="Test">
		<classes>
			<class name="tests.EUTest" />
			<class name="tests.EndUserTest" />
		</classes>
	</test> <!--Test -->
	<test thread-count="5" name="CommonSeleniumTest">
		<classes>
			<class name="commonOperations.Demo03_FormPart1" />
			<class name="commonOperations.Demo04_FormPart2" />
		</classes>
	</test>
</suite> <!--Suite -->
