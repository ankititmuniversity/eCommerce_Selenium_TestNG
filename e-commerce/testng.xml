<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="smoke"/>
			<include name="sanity"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="utils.ExtentReport"></listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url" value="https://automationexercise.com"></parameter>
	<test thread-count="5" name="e2e Test">
		<classes>
			<class name="tests.EUTest" />
			<class name="tests.EndUserTest" />
		</classes>
	</test> <!--Test -->
</suite> <!--Suite -->
